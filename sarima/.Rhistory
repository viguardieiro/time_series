par(mfrow = c(2, 2))
plot(consumo, main = "Series consumo")
diff(consumo) %>% plot(main = "Series diff(consumo)")
diff(consumo, lag = 12) %>%  plot(main = "Series diff(consumo, 12)")
diff(consumo, lag = 12) %>% diff() %>% plot(main = "Series diff(diff(consumo, 12))")
